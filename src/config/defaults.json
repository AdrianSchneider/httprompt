{
  "show.headers": true,
  "show.body": true,
  "renderer": "console",
  "external": "less",
  "external.json": "less",
  "history.length": 100,
  "default.profile": "default",
  "vim": false,
  "editor": "",
  "profiles": {
    "default": {
      "baseUrl": "",
      "requestOptions": {},
      "vars": {
        "key": "value"
      },
      "actions": {
        "login <username> <password>": [
          "POST /login { \"username\": \"$(input.username)\", \"password\": \"$(input.password)\""
        ]
      }
    }
  }
}
